              "type": "string"
            },
            "nullable": true
          },
          "message": {
            "type": "string",
            "nullable": true
          },
          "error": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "EncryptionConfig": {
        "type": "object",
        "properties": {
          "mode": {
            "type": "string",
            "nullable": true
          },
          "trustServerCertificate": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "TableInfo": {
        "type": "object",
        "properties": {
          "schema": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "fullName": {
            "type": "string",
            "nullable": true,
            "readOnly": true
          }
        },
        "additionalProperties": false
      },
      "TableListResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "tables": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TableInfo"
